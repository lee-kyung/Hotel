<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybats-3-mapper.dtd">
 
<mapper namespace="kr.co.hotel.dining.DiningMapper">
	<select id="dining_write" resultType="kr.co.hotel.dining.DiningVO">
	 select * from dining order by id asc
	</select>
	
	<insert id="dining_write_ok" parameterType="kr.co.hotel.dining.DiningVO">
	 insert into dining(dcode,dine_pimg,dine_cimg,dine_type,dine_adult,
	 dine_child,dine_halin,dine_time,dine_su,writeday)
	 values(#{dcode},#{dine_pimg},#{dine_cimg},#{dine_type},#{dine_adult},
	 #{dine_child},#{dine_halin},#{dine_time},#{dine_su},now())
	</insert>
</mapper>